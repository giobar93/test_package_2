  "scripts": {
    "build:wp": "webpack --mode production",
    "build": "yarn build:esm && yarn build:cjs && yarn copy-files:esm && yarn copy-files:cjs",
    "build:esm": "tsc",
    "copy-files:esm": "copyfiles -u 1 src/**/*.html src/**/*.css src/**/*.png dist/esm",
    "copy-files:cjs": "copyfiles -u 1 src/**/*.html src/**/*.css src/**/*.png dist/cjs",
   
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "prepublishOnly": "npm run prettier && npm run lint",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
